{
    "name": "slavkovrn/yii2-jsoneditor",
    "description": "Yii2 JsonEditor widget uses Json Editor plugin for jQuery",
    "type": "yii2-extension",
    "keywords": ["yii2", "Json Editor widget", "Json Editor plugin for jQuery"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Viacheslav Kolesnikov",
            "email": "slavko.chita@gmail.com",
            "homepage": "http://yii2.kadastrcard.ru"
        }
    ],
    "support": {
        "source": "https://github.com/SlavKoVrn/yii2-jsoneditor"
    },
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "slavkovrn\\jsoneditor\\": "src/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}